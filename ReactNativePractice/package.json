{
  "name": "ReactNativePractice",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "haul",
    "start-ios": "haul start --platform ios --dev false --minify true --port 8081",
    "start-and": "haul start --platform android --dev false --minify true --port 8081",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "build-ios": "haul bundle --platform ios --config haul.config.js --assets-dest dist --dev false"
  },
  "devDependencies": {
    "babel-preset-react-native-stage-0": "^1.0.1",
    "haul": "^1.0.0-rc.1",
    "jest": "^23.1.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "16.3.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "~0.55.2"
  }
}
